# This file exports the Eigen3::Eigen CMake target which should be passed to the
# target_link_libraries command.

@PACKAGE_INIT@

include ("${CMAKE_CURRENT_LIST_DIR}/StanMathTargets.cmake")

# Legacy variables, do *not* use. May be removed in the future.

set (STANMATH_FOUND 1)

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
check_required_components("@PROJECT_NAME@")

set (STANMATH_DEFINITIONS  "@EIGEN_DEFINITIONS@")
set (STANMATH_INCLUDE_DIR  "@PACKAGE_STANMATH_INCLUDE_DIR@")
set (STANMATH_INCLUDE_DIRS "@PACKAGE_EIGEN_INCLUDE_DIR@")
set (STANMATH_ROOT_DIR     "@PACKAGE_EIGEN_ROOT_DIR@")

set (STANMATH_VERSION_STRING "4.1.0")
set (STANMATH_VERSION_MAJOR  "4")
set (STANMATH_VERSION_MINOR  "1")
set (STANMATH_VERSION_PATCH  "0")
